{
  "name": "react-leaflet-canvas-overlay",
  "version": "0.1.3",
  "description": "React Leaflet component similar to ImageOverlay and VideoOverlay\"",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ted-piotrowski/react-leaflet-canvas-overlay.git"
  },
  "scripts": {
    "build": "yarn add -D @react-leaflet/core && tsc && yarn run minify && yarn remove @react-leaflet/core",
    "minify": "uglifyjs --compress --mangle --output dist/index.min.js -- dist/index.js"
  },
  "author": "Ted Piotrowski",
  "license": "MIT",
  "peerDependencies": {
    "leaflet": "^1.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-leaflet": "^3.0.5"
  },
  "devDependencies": {
    "@types/leaflet": "^1.5.21",
    "@types/react": "^16.8.6",
    "typescript": "^4.1.3",
    "uglify-js": "^3.12.5"
  },
  "keywords": [
    "react",
    "leaflet",
    "canvas",
    "typescript",
    "react-leaflet-v3"
  ]
}